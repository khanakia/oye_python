{
    "presets" : [
        "react",
        "es2015",
        "es2016",
        "es2017",
        "airbnb",
        "stage-0"
    ],

    "plugins" : [
        ["import", { "libraryName": "antd", "style": "css" }],
        "add-module-exports",
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-object-rest-spread", 
        "transform-do-expressions"
    ]
}
